<div class="form">
  <h3>Tasks Details</h3>
    <table class="table table-striped table-responsive">
        <thead>
          <tr>
              <th scope="col">Task Id</th>
              <th scope="col">Task Name</th>
              <th scope="col">Employee Id</th>
              <th scope="col">Task Status</th>
              <th scope="col">Assign Task</th>
          </tr>
        </thead>
        <tbody>
          <% if (sampleData.length > 0){ %>
              <% sampleData.forEach((data)=>{ %>
                  <tr>
                    <td><%= data.TaskID %></td>
                    <td><%= data.Taskname %></td>
                    <td><%= data.emp_id %></td>
                    <td><%= data.TaskStatus %></td>
                    <td>
                      <button type="button" class="btn btn-primary btn-sm"><a href="/manager/assigntask/<%= data.TaskID %>">Assign</a></button>
                    </td>
                  </tr>
                <% }) %>
          <% } else { %>
            <tr>
              <td colspan="5">No Data Found</td>
            </tr>
          <% } %>
        </tbody>
      </table>
</div>